#!/bin/bash

# usage
# callbashfunc func parameters

if [ $# -eq 0 ]
then
	exit 1
elif [ $# -eq 1 ]
then
	$1
elif [ $# -eq 2 ]
then
	$1 $2
elif [ $# -eq 3 ]
then
	$1 $2 $3
elif [ $# -eq 4 ]
then
	$1 $2 $3 $4
elif [ $# -eq 5 ]
then
	$1 $2 $3 $4 $5
elif [ $# -eq 6 ]
then
	$1 $2 $3 $4 $5 $6
elif [ $# -eq 7 ]
then
	$1 $2 $3 $4 $5 $6 $7
fi
